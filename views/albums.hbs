
<div class='artist-container'>
    {{#each albums}}
    <div class='artists' data-aos='zoom-in'>
        <div class='info'>
        <img src="{{this.images.0.url}}" alt="">
        <h3>{{this.name}}</h3>
        <button><a href="/tracks/{{this.id}}">View Tracks</a></button>
        </div>
    </div>
    {{/each}}
</div>
